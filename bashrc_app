#macvim
# if [ "Darwin" == $(uname) ]; then
#  alias vim="mvim -v"
# fi
alias sub="/Applications/Sublime\ Text\ 2.app/Contents/MacOS/Sublime\ Text\ 2"

#rvm
[[ -s "$HOME/.rvm/scripts/rvm" ]] && source "$HOME/.rvm/scripts/rvm"

#autojump
#for mannual installation
[[ -s ~/.autojump/etc/profile.d/autojump.zsh ]] && . ~/.autojump/etc/profile.d/autojump.zsh
#for apt-get install on ubuntu precise
[[ -s /usr/share/autojump/autojump.sh ]] && . /usr/share/autojump/autojump.sh

#java
JAVA_HOME="/vagrant/jdk_home"
PATH=$JAVA_HOME/bin:$PATH

#subversion on mac
SVN_HOME="/opt/subversion"
PATH=$SVN_HOME/bin:$PATH

#gradle
GRADLE_HOME="/Users/edfeng/Downloads/gradle-1.3"
PATH=$GRADLE_HOME/bin:$PATH


export PATH
