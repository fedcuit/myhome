[[ -s "$oracle_env_file_client" ]] && source "$oracle_env_file_client"

oracle_env_file_server="/usr/lib/oracle/xe/app/oracle/product/10.2.0/server/bin/oracle_env.sh"
[[ -s "$oracle_env_file_server" ]] && source "$oracle_env_file_server"
